# Variables pour Azure
location = "francecentral"  # Région Azure où déployer les ressources
prefix   = "your-prefix"    # Préfixe unique pour nommer vos ressources Azure

# Variables pour MySQL
mysql_admin_username = "your_db_user"     # Nom d'utilisateur pour la base de données MySQL
mysql_admin_password = "your_db_password"  # Mot de passe pour la base de données MySQL (à changer !)

# Variables pour GitHub
github_repo_url = "https://github.com/your-username/your-repo.git"  # URL de votre dépôt GitHub

# Variables supplémentaires pour Laravel
app_name = "Laravel"                      # Nom de votre application
app_env = "production"                    # Environnement (production pour Azure)
app_debug = "false"                       # Désactivé en production pour la sécurité
app_key = "your-base64-encoded-key"       # Clé de chiffrement Laravel (à générer avec php artisan key:generate)

# Variables pour la base de données
db_database = "your_database_name"       # Nom de votre base de données MySQL 